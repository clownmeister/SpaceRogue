.btn {
    -unity-font: resource('Fonts/Roboto-Regular');
    flex: 0 1 auto;
    min-width: 300px;
    width: auto;
    padding: 15px 30px;
    font-size: 24px;
    -unity-font-style: bold;
    border-radius: 0;
}

.btn--primary {
    background-color: #005066;
    color: #e4eff0;
    border-width: 0;
    border-radius: 10px;
}
